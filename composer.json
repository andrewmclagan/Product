{
	"name": "jiro/product",
	"description": "Jiro Product",
	"authors": [
		{
			"name": "Andrew McLagan",
			"email": "andrewmclagan@gmail.com"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"illuminate/support": "4.2.*",
		"jiro/support": "dev-master"
	},
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "phpunit/phpunit": "4.5.*"
        "laravel/laravel": "~5.0",
        "laracasts/testdummy": "~2.0"        
    },  	
	"autoload": {
		"classmap": [
			"database/migrations",
			"database/seeds"
		],
		"psr-4": {
			"Jiro\\Product\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
